{
    "name": "kooser/password-lock",
    "description": "Secure your password using password lock.",
    "homepage": "https://kooser-framework.github.io/password-lock/",
    "keywords": [],
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "symfony/options-resolver": "^4.3",
        "symfony/polyfill-mbstring": "^1.11",
        "symfony/polyfill-php72": "^1.11",
        "symfony/polyfill-php73": "^1.11"
    },
    "require-dev": {
        "vimeo/psalm": "^3",
        "phpunit/phpunit": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "Kooser\\Framework\\PasswordLock\\": "./src"
        },
        "files": [
            "./bootstrap.php"
        ]
    },
    "config": {
        "optimize-autoloader": true
    },
    "prefer-stable": true
}
